<Window x:Class="TestApp.OptieWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestApp"
        mc:Ignorable="d"
WindowState="Maximized"
WindowStyle="None"
        Title="OptieWindow" Height="450" Width="800">
    <Grid>

        <Slider x:Name="sliderEffecten" HorizontalAlignment="Left" Margin="337,147,0,248" Width="230" Maximum="100" SmallChange="1" LargeChange="10" ValueChanged="sliderEffecten_ValueChanged" />
        <Button x:Name="btnTest" Content="Button" HorizontalAlignment="Left" Margin="217,61,0,0" VerticalAlignment="Top" Click="btnTest_Click"/>
        <Button x:Name="btnTerug" Content="Button" HorizontalAlignment="Left" Margin="69,130,0,0" VerticalAlignment="Top" Click="btnTerug_Click_1"/>
        <Slider x:Name="sliderMuziek" HorizontalAlignment="Left" Margin="337,248,0,0" VerticalAlignment="Top" Width="230" ValueChanged="sliderMuziek_ValueChanged"/>

    </Grid>
    <Window.Background>
        <ImageBrush ImageSource="Optiescherm.png" Stretch="Fill" ></ImageBrush>
    </Window.Background>
</Window>
