<Window x:Class="TestApp.LevelWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestApp"
        mc:Ignorable="d"
        WindowState="Maximized"
        WindowStyle="None"
        Title="LevelWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="96*"/>
            <ColumnDefinition Width="206*"/>
            <ColumnDefinition Width="163*"/>
            <ColumnDefinition Width="335*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="71*"/>
            <RowDefinition Height="13*"/>
            <RowDefinition Height="35*"/>
            <RowDefinition Height="331*"/>
        </Grid.RowDefinitions>
        <Button Name="Back" Click="Back_Click" Grid.Row="0" Grid.Column="0" Background="Transparent" BorderBrush="Transparent"></Button>
        <Button Name="Level1" Click="Level1_Click" Grid.Row="2" Grid.Column="2" Background="Transparent"></Button>

    </Grid>
    <Window.Background>
        <ImageBrush ImageSource="Resources/LevelScherm.png" Stretch="Fill"></ImageBrush>
    </Window.Background>

</Window>
